{
  "name": "overwatch-nodecg",
  "version": "0.1.6",
  "description": "",
  "homepage": "",
  "author": {
    "name": "Russell Payne",
    "email": "payner3@etsu.edu",
    "url": "https://portfolio.ryois.me"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "ow_home",
        "title": "Overwatch Home",
        "width": 4,
        "file": "ow-home.html",
        "headerColor": "#525F78",
        "workspace": "Overwatch"
      },
      {
        "name": "ow_teams",
        "title": "Overwatch Teams",
        "width": 6,
        "file": "ow-teams.html",
        "headerColor": "#525F78",
        "workspace": "Overwatch"
      },
      {
        "name": "ow_maps",
        "title": "Overwatch Map Selection",
        "width": 6,
        "file": "ow-map-select.html",
        "headerColor": "#525F78",
        "workspace": "Overwatch"
      },
      {
        "name": "ow_new_team",
        "title": "New Team",
        "width": 3,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "save",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ],
        "file": "ow-new-team.html"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "vs.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "map_selection.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "assetCategories": [
      {
        "name": "team_logos",
        "title": "Team Logos (1750px x 450px) Do NOT align to right or left as they can be shown on either side",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      },
      {
        "name": "ads",
        "title": "Ads (270px x 75px)",
        "allowedTypes": ["png"]
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "Apache-2.0",
  "dependencies": {
    "multer": "^1.4.5-lts.1",
    "sqlite3": "^5.1.5"
  }
}
